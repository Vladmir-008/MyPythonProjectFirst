def count_numbers_with_digit_3(limit):
    count = 0
    for num in range(1, limit + 1):
        # Преобразуем число в строку и проверяем, есть ли цифра '3'
        if '3' in str(num):
            count += 1
    return count

# Устанавливаем предел
limit = 2024

# Вычисляем количество чисел
result = count_numbers_with_digit_3(limit)

# Выводим результат
print(f"Количество чисел от 1 до {limit}, содержащих хотя бы одну цифру '3': {result}")
